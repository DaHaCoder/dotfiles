# My aliases
alias ..='cd ..'
alias cd ...='cd ../..'
alias cd ....='cd ../../..'
alias ll='ls -lFh'
alias lla='ls -lAh'
alias lld='ls -lAdh .*'
alias la='ls -a'
alias ld='ls -d .*'
alias l='ls -CF'
